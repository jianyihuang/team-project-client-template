      "type": "integer"
    },
    "completed": {
      "type": "string"
    },
    "contents": {
      "type": "object",
      "properties": {
        "author": {
          "type": "string"
        },
        "subscriber": {
          "type": "string"
        },
        "date": {
          "type": "string"
        },
        "time": {
          "type": "string"
        },
