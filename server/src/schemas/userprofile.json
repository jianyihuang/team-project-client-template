{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "user_id" : {
      "type": "int",
      "id": "user_id"
    },
    "first_name": {
      "type": "string",
      "id": "first_name"
    },
    "last_name": {
      "type": "string",
      "id": "last_name"
    },
    "profilepic": {
      "type": "string",
      "id": "profilepic"
    },
    "favorite_quote": {
      "type": "string",
      "id": "favorite_quote"
    },
    "areas_of_interest": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "id": "areas_of_interest"
    },
    "classes_taken": {
      "type": "array",
      "items": {
        "type": "number"
      },
      "id": "classes_taken"
    },
    "education_level": {
      "type": "string",
      "id": "education_level"
    },
    "academic_institution": {
      "type": "string",
      "id": "academic_institution"
    }
  },
  "id": "/",
  "required": [
    "first_name",
    "last_name",
    "profilepic",
    "academic_institution",
    "education_level",
    "favorite_quote",
    "areas_of_interest",
    "classes_taken"
  ]
}
